//15th program


//16th

#include <iostream>

int main(){
std::cout << "Enter the number: ";
int num;
std::cin >> num;
while(num){
std::cout << num%10;
num /= 10;
}
std::cout << std::endl;
return 0;
}

//17th

#include <iostream>

int main(){
std::cout << "Enter the number: ";
int num,reverse,power = 1;
std::cin >> num;
while(num){
reverse += num%10 * power;
num /= 10;
power *= 10;
}
if(reverse == num){
std::cout << "The number is palindrome." << std::endl;
}else{
std::cout << "The number is not a palindrome." << std::endl;
}
return 0;
}

